select * from users;
use login_db;

USE login_db;

-- Tabela de reservas
CREATE TABLE reservas (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nome VARCHAR(100) NOT NULL,
    email VARCHAR(100) NOT NULL,
    telefone VARCHAR(15),
    data DATE NOT NULL,
    horario TIME NOT NULL,
    numero_pessoas INT NOT NULL,
    mensagem TEXT
);

select * from reservas;
